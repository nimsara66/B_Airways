<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Schedule</title>

  <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />

  <!-- font awesome cdn link  -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">

  <!-- custom css file link  -->
  <link rel="stylesheet" href="../stylesheets/schedule.css">

  <!-- boostrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

</head>

<body>
  <!-- header section starts  -->

  <%- include('../component/header.ejs',{user}); %>

    <!-- header section ends -->




    <section class="book" id="book">

      <h1 class="heading"> <span>Airplanes details</span> </h1>


      <form action="" method="POST">
        <div class="input-group">

          <input type="text" class="form-control" placeholder="modelname"
            aria-label="Recipient's username with two button addons" name="modelname">
          <input type="text" class="form-control" placeholder="varient"
            aria-label="Recipient's username with two button addons" name="varient">
          <input type="text" class="form-control" placeholder="E seat capacity"
            aria-label="Recipient's username with two button addons" name=" E_seat_cap">
          <input type="text" class="form-control" placeholder="B seat capacity"
            aria-label="Recipient's username with two button addons" name="B_seat_cap">
          <input type="text" class="form-control" placeholder="P seat capacity"
            aria-label="Recipient's username with two button addons" name="P_seat_cap">

          <input type="text" class="form-control" placeholder="E SPR"
            aria-label="Recipient's username with two button addons" name="E_SPR">
          <input type="text" class="form-control" placeholder="B SPR"
            aria-label="Recipient's username with two button addons" name="B_SPR">
          <input type="text" class="form-control" placeholder="P SPR"
            aria-label="Recipient's username with two button addons" name="P_SPR">
          <input type="text" class="form-control" placeholder="Max Weight"
            aria-label="Recipient's username with two button addons" name="max_weight">



          <button class="btn btn-outline-secondary" type="submit">Add</button>


        </div>

      </form>


      <div class="row">


        <div class="left-right">

          <table class="table table-hover">
            <thead>
              <tr>
                <th scope="col"></th>
                <th scope="col">Model Name</th>
                <th scope="col">varient</th>
                <th scope="col">E seat capacit</th>
                <th scope="col">B seat capacity</th>
                <th scope="col">P seat capacity</th>
                <th scope="col">E SPR</th>
                <th scope="col">B SPR</th>
                <th scope="col">P SPR</th>
                <th scope="col">Max Weight</th>

                <!--<th scope="col">del</th>-->
              </tr>
            </thead>
            <tbody>
              <% for(let i=0; i<aircraftModels.length; i++) { %>
                <tr>
                  <th scope="row">
                    <%= i+1 %>
                  </th>
                  <td>
                    <%= aircraftModels[i].model_name %>
                  </td>
                  <td>
                    <%= aircraftModels[i].varient %>
                  </td>
                  <td>
                    <%= aircraftModels[i].economy_seat_capacity %>
                  </td>
                  <td>
                    <%= aircraftModels[i].business_seat_capacity %>
                  </td>
                  <td>
                    <%= aircraftModels[i].platinum_seat_capacity %>
                  </td>
                  <td>
                    <%= aircraftModels[i].economy_seats_per_row %>
                  </td>
                  <td>
                    <%= aircraftModels[i].business_seats_per_row %>
                  </td>
                  <td>
                    <%= aircraftModels[i].platinum_seats_per_row %>
                  </td>

                  <td>
                    <%= aircraftModels[i].max_weight %>
                  </td>
                </tr>
                <% } %>
                  }

            </tbody>
          </table>

          <!-- <input type="submit" class="btn" value="book now"> -->


        </div>

        <!-- <div class="left-col"> -->
        <!-- <img src="images/book-img.svg" alt=""> -->
        <!-- <h1>This part contains available sheets details and passport deatails</h1> -->
        <!-- </div> -->



      </div>

    </section>


</body>

</html>